<div class="edit_form">
  <fieldset>
    <% form_for @subscription do |f| %>
      <%= f.error_messages %>
      <%= f.label :plan_id %>
      <span class="div_texbox">
        <%= f.select :plan_id, [["Free Level", "FREE_LEVEL"], ["Bronze Level", "BRONZE_LEVEL"], ["Silver Level", "SILVER_LEVEL"], ["Gold Level", "GOLD_LEVEL"]], :required => true, :class => "fillout01" %>
    	</span>
      <%= f.label :status %>
      <span class="div_texbox">
        <%= f.text_field :status, :class => "fillout01" %>
    	</span>
      <span class="div_texbox">            
        <%= f.submit "Save" %>
     	</span>
    <% end %>
  </fieldset>
</div>
