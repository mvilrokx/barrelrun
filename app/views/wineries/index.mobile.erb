<div id="home" class="current">

  <div class="toolbar">
    <h1>Wineries</h1>
    <a class="button cancel" href="/">Back</a>
  </div>

  <div class="body">
    <ul>
       <% for winery in @wineries %>
      <li class="arrow" href="#wineDetails" >
      <table>    
      <tr> 
      <td width="180"> <%= link_to image_tag("../images/page_find.png",
             :border=>0, :alt=>"Show"), winery, :title=> "Show", :class => 'dialog_form_link' %>
      </td>
      <td>
            <div><%= link_to h(winery.winery_name), winery %> </div>
            <div><%=h winery.city %>, <%=h winery.state %> </div>

            <div><%= render_stars winery%> </div>
      </td>
      </tr>
      </table>                     
      </li>
      <% end %>
    </ul>
  </div>   
</div>
