<div id="signin_menu">
<%  form_for("winery", :url => winery_session_path) do |f| %>
  <%= f.error_messages %>
   <fieldset>
      <ol>
         <li>
           <%= f.label :username %>
           <%= f.text_field :username %>
         </li>
         <li>
           <%= f.label :password %>
           <%= f.password_field :password %>
         </li>
         <li>
           <%= f.label :remember_me %>
           <%= f.check_box :remember_me %>
         </li>
      </ol>   
   </fieldset>
  <%= f.submit "Sign in" %>
<% end -%>
</div>
